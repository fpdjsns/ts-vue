{
    "compilerOptions": {
        "outDir": "./built/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "module": "es6",
        "moduleResolution": "node",
        "target": "es5",
        "experimentalDecorators": true,
        "charset": "utf8", // 입력파일의 문자집합 형식
        "emitBOM": true,
        "baseUrl": "./",
        "lib": [
            "es6",
            "dom"
        ],
        "paths": {
            "@/*": [
                "src/*"
            ],
        },
    },
    "include": [
        "./src/**/*"
    ]
}